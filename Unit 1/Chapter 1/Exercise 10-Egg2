public class Egg2 {
public static void main(String[] args) {
        top();
        bottom();
        middle();
        top();
        bottom();
        middle();
        bottom();
        top();
        middle();
        bottom();
}
public static void top(){
        System.out.println("  _______\n /       \\\n/         \\");
}
public static void bottom(){
        System.out.println("\\         /\n \\_______/");
}
public static void middle(){
        System.out.println("-\"-'-\"-'-\"-");
}
}
